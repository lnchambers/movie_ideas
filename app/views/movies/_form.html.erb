<%= form_for @movie do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :images %>
<%= f.text_field :images %>

<%= f.label :category %>
<%= f.collection_select :category_id, Category.all, :id, :name %>

<%= f.submit %>

<% end %>
