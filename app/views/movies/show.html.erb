<%= @movie.title %>
<%= @movie.description %>
Total Ratings: <%= @movie.total_rating %>
<% @movie.images.each do |image| %>
  <%= image.url %>
<% end %>

<%= link_to "Edit", edit_movie_path(@movie) %>
<%= link_to "Delete", movie_path(@movie), method: "delete" %>
<%= button_to "Upvote", movie_ratings_path(@movie), params: {"user_id" => current_user, "value" => 1} %>
