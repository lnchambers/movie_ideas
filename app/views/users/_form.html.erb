<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :ratings %>
  <%= f.hidden_field :ratings, :value => 0 %>

  <%= f.submit %>

<% end %>
